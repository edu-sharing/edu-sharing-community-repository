{
  "project": {
    "version": "1.0.0-beta.19-3",
    "name": "angular-edusharing"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets"
      ],
      "index": "index.html",
      "main": "main.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.json",
      "prefix": "app",
      "mobile": false,
      "styles": [
        "../node_modules/materialize-css/dist/css/materialize.min.css",
        "./assets/fonts/opensans_regular/stylesheet.css",
        "./assets/fonts/opensans_bold/stylesheet.css",
        "./assets/fonts/material-design-icons/material-icons.css",
        "./assets/css/nouislider.min.css",
        "./styles/custom_materialize.scss",
        "./styles/dialogs.scss",
        "./styles/file-chooser.scss",
        "./styles/globals.scss",
        "./styles/toast.scss",
        "./styles/mds.scss",
        "./styles/ng2-calendar.scss",
        "./styles/override.scss"
      ],
      "scripts": [
        "./assets/js/jquery.1.12.4.min.js",
        "./assets/js/nouislider.min.js",
        "../node_modules/materialize-css/dist/js/materialize.min.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "source": "environments/environment.ts",
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "addons": [],
  "packages": [],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "prefixInterfaces": false,
    "inline": {
      "style": false,
      "template": false
    },
    "spec": {
      "class": false,
      "component": true,
      "directive": true,
      "module": false,
      "pipe": true,
      "service": true
    }
  }
}
